{
  "extends": ["eslint:recommended", "plugin:react/jsx-runtime"],
  "env": {
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error"
  },
  "eslintIgnore": ["webpack.config.js"]
}
